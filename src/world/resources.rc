            VALUE "LegalCopyright", "AscEmu Community"
            VALUE "OriginalFilename", "world.exe"
            VALUE "ProductName", "AE World"
            VALUE "ProductVersion", AE_BUILD_HASH
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
